<section class="page-title">
  <h2><%= @artwork.name %> Add Photos</h2>
</section>

<div class="quicknav-content">
  <%= link_to  'admin dashboard', admin_dashboard_index_path %>  /  <%= link_to "#{@artwork.name}", admin_artwork_path(@artwork) %> /  <%= link_to 'my art', admin_artworks_path %>
</div><br>

<%= form_for([:admin, @artwork], url: { action: "photo_update" }) do |f| %>
  <%= f.label 'Image of Artwork: '%><br>
  <%= f.file_field :photos %><br>

  <%= f.submit 'Add Photo' %>
<% end %>
