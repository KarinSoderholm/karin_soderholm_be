<section class="page-title">
  <h1>Add a new email contact: </h1>
</section>
<%= form_tag admin_emails_path do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>

  <%= label_tag :email_address %>
  <%= email_field_tag :email_address %><br>

  <%= submit_tag 'Add Email Contact' %>
<% end %>
