<section class="page-title">
  <h2><%= @blog_post.title %> Add Photos</h2>
</section>

<div class="quicknav-content">
  <%= link_to  'admin dashboard', admin_dashboard_index_path %>  /  <%= link_to "#{@blog_post.title}", admin_blog_post_path(@blog_post) %> /  <%= link_to 'my art', admin_blog_posts_path %>
</div><br>

<%= form_for([:admin, @blog_post], url: { action: "photo_update" }) do |f| %>
  <%= f.label "Image of #{@blog_post.title}: "%><br>
  <%= f.file_field :photos %><br>

  <%= f.submit 'Add Photo' %>
<% end %>
